{
  "root": true,
  "extends": "airbnb",
  "plugins": ["react"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
    }
  },
  "env": {
      "es6": true,
      "browser": true,
  },
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "react/no-unused-prop-types": [0]
  }
}
